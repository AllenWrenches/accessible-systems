{% include "header.njk" %}
  <div class="page-wrap {{ title | slug }}">
    <div class="inside">
      <nav class="breadcrumbs" aria-label="breadcrumbs">
        <p><a href="/">Home</a> > <strong>{{title}}</strong></p>
      </nav>
      <div class="content-wrapper">
        {% block pageNav %}
          Sidebar pending
        {% endblock %}
        <main class="post" id="content">
          <h1>{{ title | capitalize }}</h1>
          {{ content | safe }}
        </main>
      </div>
    </div>
  </div>
{% include "footer.njk" %}
