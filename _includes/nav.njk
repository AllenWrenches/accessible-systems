<nav>
  <button id="toggle" aria-expanded="false">Menu</button>
  {% set navPages = collections.all | eleventyNavigation %}
  <ul id="menu">
  {%- for entry in navPages %}
    <li{% if entry.url == page.url %} class="active-page"{% endif %}>
      <a href="{{ entry.url | url }}"><span>{{ entry.title }}</span></a>
    </li>
  {%- endfor %}
  </ul>
</nav>
